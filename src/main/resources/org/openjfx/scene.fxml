                     <children>
                        <Label fx:id="label" alignment="CENTER" contentDisplay="CENTER" text="Image Searcher" textAlignment="CENTER">
                           <font>
                              <Font size="45.0" />
                           </font>
                        </Label>
                        <HBox alignment="CENTER">
                           <children>
                              <Label fx:id="label1" alignment="CENTER" contentDisplay="CENTER" text="From:" textAlignment="CENTER">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                              <JFXDatePicker fx:id="fromDate" styleClass="jfxPrimaryColor">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                              </JFXDatePicker>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <Label fx:id="label11" alignment="CENTER" contentDisplay="CENTER" text="To:" textAlignment="CENTER">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                              <JFXDatePicker fx:id="toDate" styleClass="jfxPrimaryColor" />
                           </children>
                        </HBox>
                        <Label fx:id="invalidDatesLabel" alignment="CENTER" contentDisplay="CENTER" text="Invalid Dates" textAlignment="CENTER" textFill="#f20000" visible="false">
                           <font>
                              <Font size="18.0" />
