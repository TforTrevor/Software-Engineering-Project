                        <VBox alignment="CENTER" spacing="20.0" styleClass="backgroundColorDark">
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" text="Searching for images...." textAlignment="CENTER">
                                 <font>
                                    <Font size="30.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets top="10.0" />
                                 </VBox.margin>
                              </Label>
                              <JFXSpinner styleClass="spinnerPrimaryColor" />
