   <right>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" styleClass="backgroundColorDark" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="consoleLabel" alignment="TOP_CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
