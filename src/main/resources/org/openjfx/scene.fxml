                        </JFXMasonryPane>
                     </content>
                  </ScrollPane>
                  <StackPane fx:id="imageViewerPane" styleClass="backgroundColorDark" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="100.0">
